{
	"name": "budgeted-ui",
	"version": "v1.1.1",
	"homepage": "https://budgeted.politicker.info",
	"repository": {
		"type": "git",
		"url": "git@github.com:politicker/budgeted.git"
	},
	"description": "a gui for your budget data",
	"main": "dist-electron/main/index.js",
	"debug": {
		"env": {
			"VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
		}
	},
	"scripts": {
		"build": "vite build && electron-builder --config electron-builder.yaml",
		"dev": "vite",
		"format": "prettier --write .",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"lint:unused": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix --rule 'unused-imports/no-unused-imports: \"error\"'",
		"typecheck": "tsc --noEmit",
		"migrate": "npx prisma migrate dev",
		"tag": "git commit -am $npm_package_version ; git tag $npm_package_version",
		"forge:start": "electron-forge start",
		"forge:package": "electron-forge package",
		"forge:make": "electron-forge make"
	},
	"author": "politicker",
	"license": "MIT",
	"devDependencies": {
		"@electron-forge/cli": "^7.2.0",
		"@electron-forge/maker-deb": "^7.2.0",
		"@electron-forge/maker-rpm": "^7.2.0",
		"@electron-forge/maker-squirrel": "^7.2.0",
		"@electron-forge/maker-zip": "^7.2.0",
		"@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
		"@fontsource-variable/rubik": "^5.0.20",
		"@fontsource/cutive-mono": "^5.0.18",
		"@headlessui/react": "^1.7.18",
		"@hookform/resolvers": "^3.3.4",
		"@radix-ui/react-avatar": "^1.0.4",
		"@radix-ui/react-checkbox": "^1.0.4",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-popover": "^1.0.7",
		"@radix-ui/react-select": "^2.0.0",
		"@radix-ui/react-separator": "^1.0.3",
		"@radix-ui/react-slot": "^1.0.2",
		"@tanstack/react-query": "^4.18.0",
		"@tanstack/react-table": "^8.11.3",
		"@trpc/react-query": "^10.45.0",
		"@types/eslint": "^8.56.2",
		"@types/react-dom": "^18.2.18",
		"@types/react": "^18.2.46",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@vitejs/plugin-react": "^4.2.1",
		"autoprefixer": "^10.4.16",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"cmdk": "^0.2.0",
		"date-fns": "^3.2.0",
		"electron-builder": "^24.9.1",
		"esbuild-css-modules-plugin": "^3.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"eslint": "^8.56.0",
		"next-themes": "^0.2.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prisma": "^5.7.1",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.49.3",
		"react-plaid-link": "^3.5.1",
		"react-router-dom": "^6.21.1",
		"react": "^18.2.0",
		"sonner": "^1.3.1",
		"superjson": "^2.2.1",
		"tailwind-merge": "^2.2.0",
		"tailwindcss-animate": "^1.0.7",
		"tailwindcss": "^3.4.0",
		"ts-node": "^10.9.2",
		"typescript-plugin-css-modules": "^5.0.2",
		"typescript": "^5.3.3",
		"victory": "^36.8.1",
		"vite-plugin-electron-renderer": "^0.14.5",
		"vite-plugin-electron": "^0.15.5",
		"vite": "^5.0.10"
	},
	"dependencies": {
		"@prisma/client": "^5.7.1",
		"@trpc/server": "^10.45.0",
		"electron-squirrel-startup": "^1.0.0",
		"electron-trpc": "^0.5.2",
		"plaid": "^18.2.0",
		"sqlite3": "^5.1.7-rc.0",
		"zod": "^3.22.4"
	},
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	}
}
